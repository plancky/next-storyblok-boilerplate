{
    "name": "next-storyblok",
    "version": "0.1.0",
    "private": true,
    "module": "esnext",
    "scripts": {
        "dev": "bun run dev:next & bun run dev:port-proxy",
        "dev:next": "next dev",
        "dev:port-proxy": "local-ssl-proxy --source 3010 --target 3000 --cert localhost.pem --key localhost-key.pem",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "gen-sb-types": "bun run ./storyblok/update-types.ts",
